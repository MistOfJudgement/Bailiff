{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Start AI Chat",
			"program": "${workspaceFolder}/index.js",
			"request": "launch",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "node"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Start Bot",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/bot/bot.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Run Current File",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${file}"
		}
	]
}